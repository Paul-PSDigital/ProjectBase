{
	"name": "thepauleh/ProjectBase",
	"keywords": ["zf", "doctrine", "vagrant", "monolog", "phpunit"],
	"description": "A base project with vagrant, zf2, monolog, doctrine and unit test frameworks",
	"author": {
		"name": "Paul Sellars",
		"email": "thepauleh@gmail.com",
		"homepage": "http://www.paulsellars.com"
	},
    "config": {
        "vendor-dir": "src/vendor"
    },
	"require": {
		"zendframework/zendframework": "2.2.4",
		"monolog/monolog": "1.6.0",
		"doctrine/doctrine-orm-module": "0.7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.27",
		"mockery/mockery": "0.8.0"
	},
    "autoload": {
        "psr-0": { "": "src/module/" }
    }
}